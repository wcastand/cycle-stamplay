    "stamplay",
    "driver",
    "reactive"
  ],
  "author": "William Castandet <wcastand@gmail.com> (http://wcastand.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wcastand/cycle-stamplay/issues"
  },
  "homepage": "https://github.com/wcastand/cycle-stamplay#readme",
  "devDependencies": {
    "@cycle/dom": "^9.2.2",
    "babel-cli": "^6.7.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-2": "^6.5.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0"
  },
  "dependencies": {
    "@cycle/core": "^6.0.3",
    "rx": "^4.1.0",
    "stamplay": "^1.0.5"
